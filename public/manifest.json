{
    "name": "Pasterino",
    "description": "No more pain with finding copypastas",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [{
        "matches": ["*://*.twitch.tv/*"],
        "js": ["main.js"],
        "css": ["styles.css"],
        "world": "MAIN"
    }]
}